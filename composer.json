{
    "name": "async-bot/wikipedia-plugin",
    "description": "Wikipedia plugin",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter Hordijk",
            "email": "info@pieterhordijk.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "amphp/amp": "^2.2.0",
        "amphp/http-client": "^v4.0.0-rc4",
        "amphp/log": "^1.0",
        "async-bot/core": "dev-master",
        "daverandom/exceptional-json": "^1.0",
        "opis/json-schema": "^1.0"
    },
    "require-dev": {
        "infection/infection": "^0.13.4",
        "maglnet/composer-require-checker": "^2.0",
        "phpunit/phpunit": "^8.3",
        "slevomat/coding-standard": "^5.0.4",
        "squizlabs/php_codesniffer": "^3.4.2"
    },
    "autoload": {
        "psr-4": {
            "AsyncBot\\Plugin\\Wikipedia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyncBot\\Plugin\\WikipediaTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
